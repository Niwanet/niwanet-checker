{
    "manifest_version": 3,
    "name": "Niwanet Checker",
    "author": "eaji@niwanet.net",
    "homepage_url": "https://niwanet.net",
    "description": "Vérifie si un site appartient à Niwanet",
    "version": "1.0",
    "icons": {
        "128": "images/niwanetchecker-128.png"
    },
    "action": {
        "default_popup": "page/page.html",
        "default_icon": "images/niwanetchecker.png"
    },
    "permissions": [
        "activeTab"
    ]
}